   end

--------Function 7
function MAS7()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber('15;35;10;20::13', gg.TYPE_FLOAT)
gg.getResults(gg.getResultsCount())
gg.editAll('100;100;99;99', gg.TYPE_FLOAT)
gg.clearResults()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber('4D;-1;-1;-1;-1;-1;-1;-1::29', gg.TYPE_FLOAT)
gg.refineNumber('-1', gg.TYPE_FLOAT)
gg.getResults(1)
gg.editAll('99999999999', gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber('2;0.89999997616;2;2;2;2;2::25', gg.TYPE_FLOAT)
gg.refineNumber('2', gg.TYPE_FLOAT)
gg.getResults(1)
gg.editAll('99999999999', gg.TYPE_FLOAT)
gg.clearResults()
end

