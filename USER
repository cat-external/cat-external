--------Function 1

function MAS1()
gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber('8.40779079e-45F;0D;0F;30F;50F;0.5F::33', gg.TYPE_DWORD)
    gg.refineNumber('0', gg.TYPE_DWORD)
    gg.getResults(1)-----[[Â© Dec By ãƒ¡ã…¤Já´œÊŸÊŸã…¤4Yá´á´œ äº— ]]-----
    gg.editAll('-1', gg.TYPE_DWORD)
    gg.clearResults()
    gg.toast('Aim awm')
  end


--------Function 2
function MAS2()

gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_CODE_APP)
    gg.searchNumber('h 00 00 00 00 3f 00 00 80 3e', gg.TYPE_BYTE)
    gg.getResults(100)-----[[Â© Dec By ãƒ¡ã…¤Já´œÊŸÊŸã…¤4Yá´á´œ äº— ]]-----
    gg.editAll('h 00 ec 51 b8 3d 8f c2 f5 3c', gg.TYPE_BYTE)
    gg.clearResults()
    gg.toast('Fast change Sniper')
  end


