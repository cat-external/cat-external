function MAS10()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber('Q"ingame/pickup/pickup_m24', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(11000)
gg.editAll('Q"Effects/VFX_Ingame_Laser', gg.TYPE_BYTE)
gg.clearResults()

gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber('Q"ingame/pickup/ammo/pickup_ammo_sn', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(11000)
gg.editAll('Q"effects/vfx_inagme_laser_shop', gg.TYPE_BYTE)
gg.clearResults()
end



--------Function 11 (EXIT)
function Exit()
gg.alert("»» ------------------------------------------ «« \n\n CAT-EXTERNAL\n\n»» ------------------------------------------ ««")
print(" \n ♻️ YOUTUBE : CAT-EXTERNAL  ")
gg.setVisible(true)
os.exit()
end


while true do
  if gg.isVisible(true) then
    menuk = 1
    gg.setVisible(false)
  end
  if menuk == 1 then
    START()
  end
  if menu == 2 then
    START()
    end
  menuk = -1
end
