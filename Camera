ANTENNA = "üî¥‚É¢    "
function ANTENNA1()
    if ANTENNA == "üî¥‚É¢    " then
        gg.setRanges(32)
        gg.searchNumber("h 73 6C 20 35 00 00 80 3F", 1)
        gg.getResults(gg.getResultsCount())
        gg.editAll("h 73 6C 20 35 9A 19 7A 43", 1)
        gg.clearResults()
        gg.setRanges(32)
        gg.searchNumber("h E9 C5 4A 35 00 00 80 3F", 1)
        gg.getResults(gg.getResultsCount())
        gg.editAll("h E9 C5 4A 35 9A 19 7A 43", 1)
        gg.clearResults()
        gg.toast("ANTENNA DONE ‚úîÔ∏è")
        ANTENNA = "     ‚É¢üü¢"
    elseif ANTENNA == "     ‚É¢üü¢" then
        gg.setRanges(32)
        gg.searchNumber("h 00 00 01 00 00 00 00 00 34 42 9A 99 99 3E 0A D7", 1)
        gg.getResults(gg.getResultsCount())
        gg.editAll("h C0 3f 01 00 00 00 00 00 34 42 9A 99 99 3E 0A D7", 1)
        gg.clearResults()
        gg.toast("ANTENNA OFF ‚ùå")
        ANTENNA = "üî¥‚É¢    "
    end
end

