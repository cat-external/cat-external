a = ' [ OFF ] '

function a6()
    gg.setVisible(false)
    while true do
        if gg.isVisible() then
            gg.setVisible(false)
            local savedItems = gg.getListItems()  

            if a == ' [ OFF ] ' then
gg.setRanges(32)
        gg.searchNumber("h 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 80 BF 00 00 00 00 00 00 80 BF 00 00 00 00 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 80 BF 00 00 80 7F 00 00 80 7F 00 00 80 7F 00 00 80 FF", 1)
        gg.getResults(gg.getResultsCount())
        gg.editAll("h 00 00 00 00 00 80 40 00 00 00 00 00 00 00 00 00 00 80 BF 00 00 00 00 00 00 80 BF 00 00 00 00 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 80 BF 00 00 80 7F 00 00 80 7F 00 00 80 7F 00 00 80 FF", 1)
        gg.clearResults()
           gg.toast("LEFT CAMERA ‚úîÔ∏è")
                a = ' [ ON ] '
            elseif a == ' [ ON ] ' then
                gg.toast("LEFT CAMERA ‚ùé")
                
gg.setRanges(32)
        gg.searchNumber("h 00 00 00 00 00 80 40 00 00 00 00 00 00 00 00 00 00 80 BF 00 00 00 00 00 00 80 BF 00 00 00 00 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 80 3F 00 00 00 00 00 00 00 00 00 00 80 BF 00 00 80 7F 00 00 80 7F 00 00 80 7F 00 00 80 FF", 1)
