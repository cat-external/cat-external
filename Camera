function main()
    GLabel = "Free Fire"
    GProcess = "com.dts.freefireth"
    GVersion = "1.109.1"

    gg.setVisible(false)
        CAMLEFT = CAMLEFT or "üî¥‚É¢    " 
        Airmodz = gg.choice({
        LOOTLC .. "  LOOT LOCATION",
        ANTENNA .. "  ENEMY LOCATION",
        SNIPERAIM .. "  SNIPER AIMBOT",
        SNIPERSWHICH .. "  FAST SWITCH",
        GLOOWALL .. "  INVISIBLE GLOOWALL",
        CAMLEFT .. "  LEFT CAMERA",
        "·¥áx…™·¥õ üö™"
    }, nil, "YOUR PANEL NAME ")

if Airmodz == 1 then
        LOOTLC1()
    elseif Airmodz == 2 then
        ANTENNA1()
    elseif Airmodz == 3 then
        SNIPERAIM1()
    elseif Airmodz == 4 then
        SNIPERSWHICH1()
    elseif Airmodz == 5 then
        GLOOWALL1()
    elseif Airmodz == 6 then
        a6()
    elseif Airmodz == 7 then
        a7()
    end
end


