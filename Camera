function a7()
    gg.alert("EXIT DONE")
    gg.clearResults()
    os.exit()
end
while true do
    if gg.isVisible(true) then
        XGCK = 1
        gg.setVisible(false)
    end
    if XGCK == 1 then
        main()
    end
end
